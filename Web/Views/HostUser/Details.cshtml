@model BL.ViewModels.HostUserViewModel

<table class="table table-dark">
    <tr>
        <td><b>User Name : </b></td>
        <td>@Html.DisplayFor(model => model.UserName)</td>

    </tr>
    <tr>
        <td><b>Email : </b></td>
        <td>@Html.DisplayFor(model => model.Email)</td>

    </tr>
    <tr>
        <td><b>Age : </b></td>
        <td>@Html.DisplayFor(model => model.Age)</td>

    </tr>
    <tr>
        <td><b>Address : </b></td>
        <td>@Html.DisplayFor(model => model.Address)</td>

    </tr>

</table>

<p>
    @Html.ActionLink("Edit Profile", "Edit", new { id = Model.ID }, new { @class = "btn btn-primary" })
</p>
